{
  "$schema": "https://raw.githubusercontent.com/PolymerLabs/lit-localize/master/config.schema.json",
  "sourceLocale": "en",
  "targetLocales": ["es-419", "zh_CN"],
  "tsConfig": "tsconfig.json",
  "output": {
    "mode": "runtime",
    "outputDir": "src/locales"
  },
  "interchange": {
