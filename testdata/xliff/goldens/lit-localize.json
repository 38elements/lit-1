{
  "$schema": "../../../config.schema.json",
  "sourceLocale": "en",
  "targetLocales": ["es-419", "zh_CN"],
  "tsConfig": "tsconfig.json",
  "output": {
    "mode": "runtime",
    "outputDir": "tsout",
    "exportSetLocaleFunction": true
  },
  "interchange": {
    "format": "xliff",
    "xliffDir": "xliff/"
  },
  "patches": {
    "es-419": {
      "lit": [{"before": "Mundo", "after": "Galaxia"}]
    }
  }
}
