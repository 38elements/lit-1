{
  "$schema": "https://raw.githubusercontent.com/PolymerLabs/lit-localize/master/config.schema.json",
  "sourceLocale": "en",
  "targetLocales": [
    "es-419"
  ],
  "tsConfig": "tsconfig.json",
  "tsOut": "tsout",
  "interchange": {
    "format": "xliff",
    "xliffDir": "xliff/"
  },
  "patches": {
    "es-419": {
      "a": [
        {
          "before": "Mundo",
          "after": "Galaxia"
        }
      ]
    }
  }
}
